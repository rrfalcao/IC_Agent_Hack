{
  "name": "@aweto-agent/cli",
  "version": "1.7.9",
  "description": "Scaffold a Bun agent app powered by @aweto-agent/core.",
  "repository": {
    "type": "git",
    "url": "https://github.com/awe-x402/AWEtoAgent-Kit",
    "directory": "packages/cli"
  },
  "type": "module",
  "bin": {
    "create-agent-kit": "dist/index.js"
  },
  "files": [
    "dist",
    "templates",
    "adapters"
  ],
  "scripts": {
    "test": "bun test",
    "build": "tsup",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc -p tsconfig.json --noEmit"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "agent",
    "scaffold",
    "bun",
    "awe"
  ],
  "license": "MIT",
  "dependencies": {
    "@aweto-agent/core": "^1.7.9",
    "@aweto-agent/identity": "^1.7.9",
    "viem": "^2.21.26"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "bun-types": "^1.1.16",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "@aweto-agent/eslint-config": "workspace:*",
    "@aweto-agent/prettier-config": "workspace:*"
  }
}
