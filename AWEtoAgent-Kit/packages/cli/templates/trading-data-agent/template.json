{
  "id": "trading-data-agent",
  "name": "Trading Data Agent",
  "description": "Agent that provides mock trading data via priced entrypoints (merchant agent)",
  "adapters": ["hono", "express"],
  "package": {
    "dependencies": {
      "viem": "^2.21.26"
    }
  },
  "wizard": {
    "prompts": [
      {
        "key": "AGENT_DESCRIPTION",
        "type": "input",
        "message": "How would you describe your data agent?",
        "defaultValue": "Trading data provider agent"
      },
      {
        "key": "AGENT_VERSION",
        "type": "input",
        "message": "What version should the agent start at?",
        "defaultValue": "0.1.0"
      },
      {
        "key": "AGENT_DOMAIN",
        "type": "input",
        "message": "What domain will host your agent?",
        "defaultValue": "agent.example.com"
      },
      {
        "key": "AGENT_SHORT_DESCRIPTION",
        "type": "input",
        "message": "Short description for listings (one sentence)",
        "defaultValue": "Trading data provider with on-chain identity"
      },
      {
        "key": "AGENT_SERVICE_TYPE",
        "type": "input",
        "message": "",
        "defaultValue": "API_ACCESS"
      },
      {
        "key": "AGENT_TOKEN_NAME",
        "type": "input",
        "message": "Token name (for backend factory)",
        "defaultValue": "Agent Token"
      },
      {
        "key": "AGENT_TOKEN_SYMBOL",
        "type": "input",
        "message": "Token symbol (max 10 chars)",
        "defaultValue": "AGENTA"
      },
      {
        "key": "AGENT_BACKEND_BASE_URL",
        "type": "input",
        "message": "",
        "defaultValue": "https://api-x402.world.fun"
      },
      {
        "key": "AGENT_RESOURCE_LINK",
        "type": "input",
        "message": "Primary resource link",
        "defaultValue": "https://{{AGENT_DOMAIN}}"
      },
      {
        "key": "AGENT_ACCESS_DETAILS",
        "type": "input",
        "message": "Access details (optional, defaults to agent domain)",
        "defaultValue": ""
      },
      {
        "key": "AGENT_GITHUB_LINK",
        "type": "input",
        "message": "GitHub link (optional)",
        "defaultValue": ""
      },
      {
        "key": "AGENT_TWITTER_LINK",
        "type": "input",
        "message": "Twitter/X link (optional)",
        "defaultValue": ""
      },
      {
        "key": "AGENT_DOCUMENT_LINK",
        "type": "input",
        "message": "Documentation link (optional)",
        "defaultValue": ""
      },
      {
        "key": "PAYMENTS_FACILITATOR_URL",
        "type": "input",
        "message": "",
        "defaultValue": "https://facilitator.world.fun/"
      },
      {
        "key": "PAYMENTS_NETWORK",
        "type": "input",
        "message": "",
        "defaultValue": "base"
      },
      {
        "key": "PAYMENTS_DEFAULT_PRICE",
        "type": "input",
        "message": "Default price for entrypoints (micro USDC 1000 -> 0.001 USDC)",
        "defaultValue": "5000"
      },
      {
        "key": "RPC_URL",
        "type": "input",
        "message": "",
        "defaultValue": "https://mainnet.base.org"
      },
      {
        "key": "CHAIN_ID",
        "type": "input",
        "message": "",
        "defaultValue": "8453"
      },
      {
        "key": "AGENT_METADATA_URI",
        "type": "input",
        "message": "Metadata URI (optional)",
        "defaultValue": "https://{{AGENT_DOMAIN}}/.well-known/agent-metadata.json"
      },
      {
        "key": "AGENT_CARD_URI",
        "type": "input",
        "message": "Agent card URI (optional)",
        "defaultValue": "https://{{AGENT_DOMAIN}}/.well-known/agent-card.json"
      },
      {
        "key": "PRIVATE_KEY",
        "type": "input",
        "message": "Agent wallet private key (leave blank to auto-generate)",
        "defaultValue": ""
      }
    ]
  }
}
