{
  "name": "@aweto-agent/core",
  "version": "1.7.9",
  "description": "Build typed agent HTTP apps",
  "repository": {
    "type": "git",
    "url": "https://github.com/awe-x402/AWEtoAgent-Kit",
    "directory": "packages/core"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "default": "./dist/utils.js"
    },
    "./axllm": {
      "types": "./dist/axllm/index.d.ts",
      "import": "./dist/axllm/index.js",
      "default": "./dist/axllm/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "AGENTS.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "zod": "^4.1.12",
    "hono": "4.10.1",
    "@aweto-agent/types": "^1.3.0",
    "@aweto-agent/a2a": "^0.2.0",
    "@aweto-agent/ap2": "^0.2.0",
    "@aweto-agent/identity": "^1.7.9",
    "@aweto-agent/payments": "^1.7.9",
    "@aweto-agent/wallet": "^0.2.1",
    "x402-fetch": "^0.7.1",
    "x402": "^0.7.1",
    "viem": "^2.38.5",
    "@ax-llm/ax": "^14.0.37",
    "@ax-llm/ax-tools": "^14.0.37"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "@aweto-agent/eslint-config": "workspace:*",
    "@aweto-agent/prettier-config": "workspace:*",
    "@aweto-agent/hono": "^0.6.0"
  }
}
