{
  "name": "@aweto-agent/types",
  "version": "1.3.1",
  "description": "Shared types for AWEtoAgent framework",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js",
      "default": "./dist/core/index.js"
    },
    "./a2a": {
      "types": "./dist/a2a/index.d.ts",
      "import": "./dist/a2a/index.js",
      "default": "./dist/a2a/index.js"
    },
    "./ap2": {
      "types": "./dist/ap2/index.d.ts",
      "import": "./dist/ap2/index.js",
      "default": "./dist/ap2/index.js"
    },
    "./identity": {
      "types": "./dist/identity/index.d.ts",
      "import": "./dist/identity/index.js",
      "default": "./dist/identity/index.js"
    },
    "./payments": {
      "types": "./dist/payments/index.d.ts",
      "import": "./dist/payments/index.js",
      "default": "./dist/payments/index.js"
    },
    "./wallets": {
      "types": "./dist/wallets/index.d.ts",
      "import": "./dist/wallets/index.js",
      "default": "./dist/wallets/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/awe-x402/AWEtoAgent-Kit",
    "directory": "packages/types"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "type-check": "tsc -p tsconfig.json --noEmit"
  },
  "peerDependencies": {
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "zod": "^4.1.12",
    "x402": "^0.7.1"
  }
}
